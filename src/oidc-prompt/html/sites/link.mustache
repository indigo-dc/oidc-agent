{{> text}}
<h3>
    <a href="{{label}}">{{label}}</a>
</h3>
<button class="btn btn-primary" onclick="window.openLink('{{label}}')">Open in default browser</button>

{{#img-data}}
    <div class="mt-2">
        <img src="{{img-data}}">
    </div>
{{/img-data}}

<div class="mt-2">
    <button onclick="window.terminate()" class="btn btn-success">OK</button>
</div>

{{>timeout}}